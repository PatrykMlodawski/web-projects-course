{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "prefer-destructuring": 0,
    "no-param-reassign": 0,
    "guard-for-in": 0,
    "no-restricted-syntax": 0,
    "no-shadow": 0
  }
}
